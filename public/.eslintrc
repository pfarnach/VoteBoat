{
  "extends": "airbnb",
  "rules": {
    "arrow-body-style": 0,
    "arrow-parens": 0,
    "max-len": [2, {"code": 100, "ignoreComments": true, "ignoreStrings": true}],
    "no-console": [2, {"allow": ["warn", "error"]}],
    "react/jsx-filename-extension": 0
  },
  "globals": {
    "expect": false
  }
}
